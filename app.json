{
  "expo": {
    "name": "foap",
    "slug": "foap",
    "scheme": "https://expo.dev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./src/assets/images/app/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hizup.foap",  
      "jsEngine": "hermes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9e51a0c0-39bb-4424-a250-6e9e3f6f9f2f"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
